{
  "name": "inotebook",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "frontend": "npm run dev --prefix frontend",
    "server": "nodemon --env-file backend/.env backend/index.js",
    "start": "concurrently \"npm run frontend\" \"npm run server\"",
    "build": "npm run build --prefix frontend",
    "preview": "npm run preview --prefix frontend"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "git": "^0.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
